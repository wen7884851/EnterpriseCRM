    <!-- XML -->
    <!--<errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="~/ElmahLogs" />-->
    <errorLog type="Elmah.XmlFileErrorLog, Elmah" logPath="F:/ElmahLogs" />
    <errorFilter>
      <test>
        <equal binding="HttpStatusCode" value="404" type="Int32" />
        <!--<equal binding="HttpStatusCode" value="500" type="Int32" />-->
      </test>
    </errorFilter>
    <!--<security allowRemoteAccess="false" />-->
  </elmah>
