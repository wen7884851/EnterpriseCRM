<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：开发系统相关配置 ================== -->
  <!-- 服务器缓存类型:WebCache、Redis-->
  <add key="CacheType" value="WebCache" />
  <!-- 单点登录Api路径 -->
  <add key="ApiLonginUrl" value="http://124.232.153.59:113/api/ApiLogin"/>
  <add key="ApiBasenUrl" value="http://t.hnmcxx.com/api/WxYY"/>
  <!-- 登陆提供者模式：Session、Cookie-->
  <add key="LoginProvider" value="Cookie" />
  <!-- 启用系统日志-->
  <add key="IsLog" value="true" />
  <!-- 数据库超时间-->
  <add key="CommandTimeout" value="180" />
  <!--启用IP过滤 -->
  <add key="IsIPFilter" value="false" />
  <!--目录路径，可以是相对路径或绝对路径-->
  <add key="FileLog" value="D:\PubSer_log"/>
  <!-- ================== 2：系统软件参数配置 ================== -->
  <!-- 软件名称 -->
  <!--<add key="SoftName" value="" />-->
  <!-- 软件版本 -->
  <add key="Version" value="1.0" />
  <!-- 软件授权码 测试备用 -->
  <add key="LicenceKey" value="83eda07f-ead3-4050-bcee-4f41e57a7e16" />
  <!-- ================== 3：外部邮件参数 ================== -->
  <!-- 设置邮箱名称 -->
  <add key="MailName" value="邮件中心" />
  <!-- 启用邮箱 -->
  <add key="ErrorToMail" value="true" />
  <!-- 设置邮箱地址 -->
  <add key="MailUserName" value="" />
  <!-- 设置邮箱密码 -->
  <add key="MailPassword" value="" />
  <!-- 设置邮箱主机 -->
  <add key="MailHost" value="smtp.qq.com" />
  <!--文件存储路径-->
  <add key="IDCardPhotoUrl" value="D:\Photo\IDCard\"></add>
  <add key="PersonPhotoUrl" value="D:\Photo\Person\"></add>
  <add key="ZipUrl" value="D:\Photo\Zip\"></add>
  <!-- 设置系统名称 -->
  <add key="SystemName" value="公众服务平台后台" />
  <add key="Configs\system.config" value="Configs\system.config"/>
  <add key="webpages:Version" value="3.0.0.0" />
  <add key="webpages:Enabled" value="false" />
  <add key="ClientValidationEnabled" value="true" />
  <add key="UnobtrusiveJavaScriptEnabled" value="true" />


  
  <!-- Close Browser Link -->
  <add key="vs:EnableBrowserLink" value="false" />
  <add key="elmah.mvc.disableHandler" value="false" />
  <add key="elmah.mvc.disableHandleErrorFilter" value="false" />
  <add key="elmah.mvc.requiresAuthentication" value="false" />
  <add key="elmah.mvc.IgnoreDefaultRoute" value="false" />
  <add key="elmah.mvc.allowedRoles" value="*" />
  <add key="elmah.mvc.allowedUsers" value="*" />
  <add key="elmah.mvc.route" value="elmah" />
</appSettings>
