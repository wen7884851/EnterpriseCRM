<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="CreateModuleModalLabel" id="CreateModuleModal">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="createModalLabel">创建模块</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-12" for="ModuleName">模块名称</label>
                            <div class="col-xs-12">
                                <input class="form-control" type="text" id="ModuleName" placeholder="模块名称..">
                                <div class="help-block" id="ModuleNameErrorMsg" style="color:red"></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-12" for="isChildModule">是否子集菜单</label>
                            <label class="lyear-switch switch-solid switch-primary">
                                <input type="checkbox" checked="" onchange="IsChildModuleChange()" id="isChildModule">
                                <span></span>
                            </label>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-12" for="LinkPerson">项目联系人</label>
                            <div class="col-xs-12">
                                <input class="form-control" type="text" id="LinkPerson" placeholder="项目联系人..">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-12" for="LinkPhoneNo">联系电话</label>
                            <div class="col-xs-12">
                                <input class="form-control" type="text" id="LinkPhoneNo" placeholder="联系电话.." onkeypress="return event.keyCode>=4&&event.keyCode<=57">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-12" for="TotalCost">项目总造价</label>
                            <div class="col-xs-12">
                                <div class="input-group m-b-10">
                                    <input class="form-control" type="text" id="TotalCost" placeholder="项目总造价.." onkeyup="this.value=clearNoNum(this.value)">
                                    <span class="input-group-addon">元</span>
                                </div>
                                <div class="help-block" id="TotalCostErrorMsg" style="color:red"></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="col-xs-12" for="ContractMoney">项目合同金额</label>
                            <div class="col-xs-12">
                                <div class="input-group m-b-10">
                                    <input class="form-control" type="text" id="ContractMoney" placeholder="项目合同金额.." onkeyup="this.value=clearNoNum(this.value)">
                                    <span class="input-group-addon">元</span>
                                </div>
                                <div class="help-block" id="ContractMoneyErrorMsg" style="color:red"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-xs-12" for="Address">项目地址</label>
                            <div class="col-xs-12">
                                <input class="form-control" type="text" id="Address" placeholder="项目地址..">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-xs-12" for="Content">项目内容</label>
                            <div class="col-xs-12">
                                <textarea class="form-control" id="Content" rows="6" placeholder="内容.."></textarea>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" onclick="closeCreateProjectModal()">关闭</button>
                <button class="btn btn-label btn-primary" onclick="CreateProject()"><label><i class="mdi mdi-checkbox-marked-circle-outline"></i></label> 创建</button>
            </div>
        </div>
    </div>
</div>

