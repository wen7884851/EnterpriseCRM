<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header"><h4>用户列表</h4></div>
            <div class="card-toolbar clearfix">
                <div class="col-lg-2">
                    <div class="form-group">
                        <label for="LoginName">账户名</label>
                        <input class="form-control" type="text" id="LoginName" placeholder="账户名..">
                    </div>
                </div>
                <div class="col-lg-2">
                    <div class="form-group">
                        <label for="FullName">姓名</label>
                        <input class="form-control" type="text" id="FullName" placeholder="姓名..">
                    </div>
                </div>
                <div class="toolbar-btn-action" style="float:right">
                    <a class="btn btn-success m-r-5" href="#!" id="searchBtn" onclick="Search(1)"><i class="mdi mdi-magnify"></i> 查询</a>
                    <button type="button" class="btn btn-primary m-r-5" data-toggle="modal" onclick="OpenCreateUserModal();">
                        <i class="mdi mdi-plus"></i>新增
                    </button>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="tablelist">
                        <thead>
                            <tr>
                                <th>用户名</th>
                                <th>姓名</th>
                                <th>邮箱地址</th>
                                <th>联系电话</th>
                                <th>最后一次登录时间</th>
                                <th>系统角色</th>
                                <th>操作</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <ul class="pagination" id="filter" style="float:right"></ul>
                <ul class="clear" style="clear:both"></ul>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/new/tablequery/tableList.js"></script>
<script src="~/Scripts/new/Core/user.js"></script>